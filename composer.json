{
    "name":        "arcanedev/laravel-nestedset",
    "description": "Nested Set Model for Laravel",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "nested", "tree", "database", "hierarchy", "categories", "menus"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelNestedSet",
    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        },{
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                 ">=5.5.9",
        "arcanedev/support":   "~3.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "phpunit/phpcov":      "~2.0|~3.0",
        "phpunit/phpunit":     "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelNestedSet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelNestedSet\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
